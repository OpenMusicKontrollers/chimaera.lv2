@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .

@prefix chim:	<http://open-music-kontrollers.ch/lv2/chimaera#> .

chim:dummy_in
	a lv2:Plugin ;
	lv2:binary <chimaera@LIB_EXT@> ;
	rdfs:seeAlso <chimaera.ttl> .

chim:tuio2_in
	a lv2:Plugin ;
	lv2:binary <chimaera@LIB_EXT@> ;
	rdfs:seeAlso <chimaera.ttl> .

chim:dump_in
	a lv2:Plugin ;
	lv2:binary <chimaera@LIB_EXT@> ;
	rdfs:seeAlso <chimaera.ttl> .

chim:filter
	a lv2:Plugin ;
	lv2:binary <chimaera@LIB_EXT@> ;
	rdfs:seeAlso <chimaera.ttl> .

chim:mapper
	a lv2:Plugin ;
	lv2:binary <chimaera@LIB_EXT@> ;
	rdfs:seeAlso <chimaera.ttl> .

chim:control_out
	a lv2:Plugin ;
	lv2:binary <chimaera@LIB_EXT@> ;
	rdfs:seeAlso <chimaera.ttl> .

chim:midi_out
	a lv2:Plugin ;
	lv2:binary <chimaera@LIB_EXT@> ;
	rdfs:seeAlso <chimaera.ttl> .

chim:osc_out
	a lv2:Plugin ;
	lv2:binary <chimaera@LIB_EXT@> ;
	rdfs:seeAlso <chimaera.ttl> .

chim:injector
	a lv2:Plugin ;
	lv2:binary <chimaera@LIB_EXT@> ;
	rdfs:seeAlso <chimaera.ttl> .

chim:simulator
	a lv2:Plugin ;
	lv2:binary <chimaera@LIB_EXT@> ;
	ui:ui chim:simulator_ui ;
	rdfs:seeAlso <chimaera.ttl> .

chim:visualizer
	a lv2:Plugin ;
	lv2:binary <chimaera@LIB_EXT@> ;
	ui:ui chim:visualizer_ui ;
	rdfs:seeAlso <chimaera.ttl> .

chim:simulator_ui
	a ui:X11UI ;
	ui:binary <chimaera_ui@LIB_EXT@> ;
	rdfs:seeAlso <chimaera.ttl> .

chim:visualizer_ui
	a ui:X11UI ;
	ui:binary <chimaera_ui@LIB_EXT@> ;
	rdfs:seeAlso <chimaera.ttl> .
